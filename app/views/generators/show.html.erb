<h1><%= @generator.name %></h1>

<div class="tab">
<label>Tags</label>
<span class="tab-content">
<% @generator.tags.each do |tag| %>
  <span class="tag">
    <%= link_to tag.name, tag_url(tag) %>
  </span>
<% end %>
</span>
</div>

<div class="tab">
<label>Preview</label>
<span class="tab-content">
<%= @generator.preview %>
</span>
</div>

<div class="tab">
<label>Model</label>
<span class="tab-content">
<pre>
<%= JSON.pretty_generate(@generator.model) %>
</pre>
</span>
</div>